    <div class="col-sm-6">
        <div class="form-group">
            <label for="lastname">Rechercher</label>
            <input id="myInput" type="text" class="form-control" name="rechercher" placeholder="rechercher ...">
        </div>
    </div>

    <table cellpadding="2" cellspacing="2" border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>picture</th>
                <th>firstName</th>
                <th>lastName</th>
                <th>email</th>
                <th>username</th>
                <th>password</th>
                <th>telephone</th>
                <th>gender</th>
                <th>adresse</th>
                <th>role</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody id="myTable">
            @foreach (var p in ViewBag.result)
            {

                <tr>
                    <td>@p.id</td>
                    <td>@p.picture</td>
                    <td>@p.firstName</td>
                    <td>@p.lastName</td>
                    <td>@p.email</td>
                    <td>@p.username</td>
                    <td>@p.password</td>
                    <td>@p.telephone</td>
                    <td>@p.gender</td>
                    <td>@p.adresse</td>
                    <td>@p.role</td>
                    <td>

                        <!-- <li><a class="" href="http://localhost:8084/secure/auth/signaler/@p.id">Signaler</a></li> -->
                        @Html.ActionLink("Block User",  "Block", new { id = @p.id }) |
                        @Html.ActionLink("Unlock User", "Signaler", new { id = @p.id })

                    </td>
                </tr>

            }
        </tbody>
    </table>

